{
    "name": "ivory/google-map",
    "description": "Google Map API v3 integration",
    "keywords": [ "google", "map"],
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel Breu",
            "email": "samuel.breu@bresam.ch",
            "role": "Developer/Maintainer"
        },
        {
            "name": "Eric GELOEN",
            "email": "geloen.eric@gmail.com",
            "role": "Creator/Inactive"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ivory/json-builder": "^3.0",
        "symfony/event-dispatcher": "^4.0 || ^5.0",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "ivory/serializer": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.2",
        "phpunit/phpunit": "^8.0",
        "phpunit/phpunit-selenium": "^8.0",
        "symfony/cache": "^4.0 || ^5.0",
        "symfony/phpunit-bridge": "^4.0 || ^5.0",
        "guzzlehttp/guzzle": "^7.3.0",
        "caseyamcl/guzzle_retry_middleware": "^2.6"
    },
    "suggest": {
        "ivory/serializer": "Allows to use http services",
        "php-http/client-implementation": "Allows to use http services",
        "php-http/message": "Allows to use http services"
    },
    "replace": {
        "egeloen/google-map": "^2.0.2"
    },
    "autoload": {
        "psr-4": { "Ivory\\GoogleMap\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Ivory\\Tests\\GoogleMap\\": "tests/" }
    }
}
